<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.DevLevelChoose">




    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/vibe_post_top_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#D3E4CD"
        android:layout_marginTop="0dp"
        android:paddingHorizontal="15dp"
        android:paddingTop="20dp"
        android:paddingBottom="16dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-16dp">

        <ImageButton
            android:id="@+id/start_back_btn_dev_level"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="center_vertical"
            android:background="@color/my_primary"
            android:contentDescription="@string/app_name"
            android:src="@drawable/baseline_arrow_back_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/title_name_question"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="개발 인원 &amp; 수준"
            android:textColor="@color/black"
            android:textSize="19sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageButton
            android:id="@+id/arrow_move_forward_dev_level"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="center_vertical"
            android:background="@color/my_primary"
            android:contentDescription="@string/app_name"
            android:src="@drawable/baseline_arrow_forward_24"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="@id/vibe_post_top_bar_layout"
            app:layout_constraintTop_toTopOf="@id/vibe_post_top_bar_layout" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- Number of developers selection -->


    <EditText
        android:id="@+id/personcount"
        android:layout_width="185dp"
        android:layout_height="36dp"
        android:layout_marginTop="48dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.57"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vibe_post_top_bar_layout" />

    <EditText
        android:id="@+id/person1"
        android:layout_width="133dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@color/white"
        app:layout_constraintStart_toEndOf="@+id/icoperson1"
        app:layout_constraintTop_toTopOf="@+id/icoperson1" />

    <ImageView
        android:id="@+id/icoperson1"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginTop="136dp"
        android:background="@drawable/person_ico"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vibe_post_top_bar_layout" />

    <ImageButton
        android:id="@+id/mid_button1"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_mid_selector"

        app:layout_constraintStart_toEndOf="@id/low_button1"
        app:layout_constraintTop_toTopOf="@id/person1" />

    <ImageButton
        android:id="@+id/low_button1"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/level_button_low_selector"

        app:layout_constraintStart_toEndOf="@id/person1"
        app:layout_constraintTop_toTopOf="@id/person1" />

    <ImageButton
        android:id="@+id/high_button1"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:background="@drawable/level_button_high_selector"
        app:layout_constraintStart_toEndOf="@id/mid_button1"
        app:layout_constraintTop_toTopOf="@id/person1" />

    <!-- Second person -->
    <ImageView
        android:id="@+id/icoperson2"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginTop="10dp"
